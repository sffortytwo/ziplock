{
  "manifest_version": 2,
  "name": "URLInterceptor",
  "version": "1.0",
  "description": "This extension intercepts URLs before they are loaded.",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "notifications"],
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  }
}
